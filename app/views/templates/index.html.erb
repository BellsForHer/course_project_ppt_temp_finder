<h1>Templates#index</h1>

<% @templates.each do |template| %>
    <h2><%= template.title %></h2>
    <ul>
    <li><%= template.tag1 %></li>
    <li><%= template.tag2 %></li>
    <li><%= template.tag3 %></li>
    </ul>
    

<% end %>

<% form_tag templates_path, :method => 'get' do %>
    <p> 
        <%= text_field_tag :search, params[:search]%>
        <%= submit_tag "search", :name => nil %>
    </p>
<% end %>

